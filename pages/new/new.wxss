/* new.wxss */
.map-wrap {
  height: 40%;
  overflow: hidden;
  box-shadow: 0 10px 50px -10px #999;
}

.map-wrap #map {
  height: 100%;
  width: 100%;
}

.new-wrap {
  height: 60%;
}
.new-panel-wrap {
  margin: 30px 10px;
  box-sizing: border-box;
}
.new-panel {
  position: relative;
  padding: 15px 10px;
  background: #fff;
  margin: 20px 0;
  border-radius: 5px;
  box-shadow: 5px 5px 30px -10px #999;
}

.zan-cell picker {
  width: 100%;
}